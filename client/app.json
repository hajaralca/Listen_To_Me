{
  "expo": {
    "name": "Listen To Me",
    "slug": "listen-to-me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "listen-to-me",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sawoty.listentome",
      "googleServicesFile": "./google-services.json" 
    },
    "ios": {
      "bundleIdentifier": "com.sawoty.listentome",
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "93d265ae-572b-4dcb-b050-7827add7dcd2"
      }
    },
    "owner": "hajaralca"
  }
}
